    "@prisma/client": "2.17.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "connected-next-router": "^4.0.1",
    "crypto-js": "^4.0.0",
    "framer-motion": "^3.6.7",
    "js-base64": "^3.6.0",
    "moment": "^2.29.1",
