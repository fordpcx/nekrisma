{
  "name": "nekrisma-framework",
  "version": "0.1.0",
  "private": true,
  "main": "desktop",
  "scripts": {
    "start": "next build web && next start web",
    "dev": "npm run lint && next dev web",
    "build": "npm run clean && next build web",
    "static-build": "npm run build && next export web",
    "prisma": "npx prisma studio",
    "prisma-dev": "next dev & npm run prisma",
    "prisma-migrate": "npx prisma migrate dev --preview-feature",
    "desktop": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "desktop-build": "npm run build && electron-builder",
    "clean": "rimraf dist web/.next web/out",
    "lint": "eslint --fix && eslint --max-warnings 1 . && echo 'âœ”  Your code looks good.'"
  },
